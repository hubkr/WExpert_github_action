    <!-- #region ImageButtonStyle Style -->
    <Style x:Key="ImageButtonStyle" TargetType="Button">
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid x:Name="ButtonRoot"
                              CornerRadius="{Binding (helper:ImageButtonHelper.CornerRadius), RelativeSource={RelativeSource TemplatedParent}}"
                              Width="{Binding (helper:ImageButtonHelper.ButtonWidth), RelativeSource={RelativeSource TemplatedParent}}"
                              Height="{Binding (helper:ImageButtonHelper.ButtonHeight), RelativeSource={RelativeSource TemplatedParent}}">

                        <Image x:Name="ButtonImage"
                                       Width="{Binding (helper:ImageButtonHelper.ImageWidth), RelativeSource={RelativeSource TemplatedParent}}"
                                       Height="{Binding (helper:ImageButtonHelper.ImageHeight), RelativeSource={RelativeSource TemplatedParent}}"
                                       Source="{Binding (helper:ImageButtonHelper.NSource), RelativeSource={RelativeSource TemplatedParent}}"/>

                        <VisualStateManager.VisualStateGroups>
                            <VisualStateGroup x:Name="CommonStates">
                                <!-- Normal 상태 -->
                                <VisualState x:Name="Normal">
                                    <Storyboard>
                                        <!-- Source -->
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ButtonImage" Storyboard.TargetProperty="Source">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                            Value="{Binding (helper:ImageButtonHelper.NSource), RelativeSource={RelativeSource TemplatedParent}}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <!-- Background -->
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ButtonRoot" Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0"
                                                                            Value="{Binding (helper:ImageButtonHelper.NBackground), RelativeSource={RelativeSource TemplatedParent}}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>

                                <!-- Hover 상태 -->
                                <VisualState x:Name="PointerOver">
                                    <Storyboard>
                                        <!-- Source -->
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ButtonImage" Storyboard.TargetProperty="Source">
                                            <DiscreteObjectKeyFrame KeyTime="0" 
                                                                            Value="{Binding (helper:ImageButtonHelper.HSource), RelativeSource={RelativeSource TemplatedParent}}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <!-- Background -->
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ButtonRoot" Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{Binding (helper:ImageButtonHelper.HBackground), RelativeSource={RelativeSource TemplatedParent}}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>

                                <!-- Pressed 상태 -->
                                <VisualState x:Name="Pressed">
                                    <Storyboard>
                                        <!-- Source -->
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ButtonImage"
                                                                           Storyboard.TargetProperty="Source">
                                            <DiscreteObjectKeyFrame KeyTime="0" 
                                                                            Value="{Binding (helper:ImageButtonHelper.PSource), RelativeSource={RelativeSource TemplatedParent}}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <!-- Background -->
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ButtonRoot" Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{Binding (helper:ImageButtonHelper.PBackground), RelativeSource={RelativeSource TemplatedParent}}" />
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>

                                <!-- Disabled 상태 -->
                                <VisualState x:Name="Disabled">
                                    <Storyboard>
                                        <!-- Source -->
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ButtonImage"
                                                                           Storyboard.TargetProperty="Source">
                                            <DiscreteObjectKeyFrame KeyTime="0" 
                                                                            Value="{Binding (helper:ImageButtonHelper.DSource), RelativeSource={RelativeSource TemplatedParent}}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                        <!-- Background -->
                                        <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ButtonRoot" Storyboard.TargetProperty="Background">
                                            <DiscreteObjectKeyFrame KeyTime="0" Value="{Binding (helper:ImageButtonHelper.DBackground), RelativeSource={RelativeSource TemplatedParent}}"/>
                                        </ObjectAnimationUsingKeyFrames>
                                    </Storyboard>
                                </VisualState>
                            </VisualStateGroup>
                        </VisualStateManager.VisualStateGroups>
                    </Grid>


                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
