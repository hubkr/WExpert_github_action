    <SolidColorBrush x:Key="WEXLineTextBoxSelectionHighlightColor1" Color="{StaticResource TypeTransparent30}"/>

    <Thickness x:Key="WEXLineTextBoxBorderThemeThicknessFocused1">1</Thickness>
    <Thickness x:Key="WEXLineTextBoxBorderThemeThickness1">1</Thickness>

    <Style x:Key="WEXLineTextBoxStyle1" TargetType="TextBox">
        <Setter Property="IsSpellCheckEnabled" Value="False"/>
        <Setter Property="FontFamily" Value="{StaticResource DefaultFont}"/>
        <Setter Property="FontWeight" Value="Normal" />
        <Setter Property="SelectionHighlightColor" Value="{StaticResource WEXLineTextBoxSelectionHighlightColor1}"/>
        <Setter Property="Foreground" Value="{StaticResource WEXLineTextBoxForeground1}" />
        <Setter Property="Background" Value="{StaticResource WEXLineTextBoxBackground1}" />
        <Setter Property="BorderBrush" Value="{StaticResource WEXLineTextBoxBorderBrush1}" />
        <Setter Property="BorderThickness" Value="{StaticResource WEXLineTextBoxBorderThemeThickness1}" />
        <Setter Property="FontSize" Value="{ThemeResource ControlContentThemeFontSize}" />
