    <SolidColorBrush x:Key="WEXLineTextBoxPlaceholder1" Color="{StaticResource SolidLtSolid}"/>
    <SolidColorBrush x:Key="WEXLineTextBoxPlaceholderPointerOver1" Color="{StaticResource SolidLtSolid}"/>
